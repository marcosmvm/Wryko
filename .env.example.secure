# Secure Environment Variables Template
# Copy to .env.local and fill with your actual keys

# App Configuration
NEXT_PUBLIC_APP_URL=http://localhost:3000
NODE_ENV=development

# Supabase (Get new keys from dashboard)
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here

# Stripe (Generate new keys)
STRIPE_SECRET_KEY=sk_test_your_new_key_here
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_your_new_key_here
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret_here

# N8N (Create new credentials)  
N8N_API_KEY=your_new_n8n_key_here
N8N_WEBHOOK_URL=https://your-n8n-instance.app.n8n.cloud/webhook

# External APIs (All need new keys)
APOLLO_API_KEY=your_new_apollo_key_here
INSTANTLY_API_KEY=your_new_instantly_key_here
OPENROUTER_API_KEY=your_new_openrouter_key_here
GOOGLE_CLOUD_API_KEY=your_new_gcp_key_here

# Email
RESEND_API_KEY=your_resend_key_here
FROM_EMAIL=noreply@wryko.com

# Security
NEXTAUTH_SECRET=generate_new_random_secret
JWT_SECRET=generate_new_jwt_secret